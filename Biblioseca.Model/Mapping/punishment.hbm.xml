<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  assembly="Biblioseca.Model"
  namespace="Biblioseca.Model">
  <class name="Punishment" table="Punishments">
    <id name="Id">
      <generator class="identity" />
    </id>
    <many-to-one name="Partner" class="Partner">
	    <column name="PartnerId" sql-type="int" not-null="true" />
	</many-to-one>
	<many-to-one name="Loan" class="Loan">
		<column name="LoanId" sql-type="int" not-null="true" />
	</many-to-one>
  </class>
</hibernate-mapping>